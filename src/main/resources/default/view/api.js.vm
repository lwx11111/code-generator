import request from '@/utils/request'
const serverName = '/shop'

#if(${cfg.prePath})
  #set($restUrl="/"+$cfg.prePath+"/"+${table.entityPath})
#else
  #set($restUrl="/"+${table.entityPath})
#end
#set($restUrl=$restUrl.toLowerCase())
#set($entryLower=$entity.toLowerCase())
#set($viewModuleNameLower=$cfg.viewModuleName.toLowerCase())

export default {

  // 分页查询
  selpage4${entryLower}(data) {
    return request({
      url: serverName + '/${restUrl}/selpage',
      method: 'post',
      data: data
    })
  },

  // 添加
  add4${entryLower}(obj) {
    return request({
      url: serverName + '/${restUrl}',
      method: 'post',
      data: obj
    })
  },

  // 修改
  update4${entryLower}(id, obj) {
    return request({
      url: serverName + '/${restUrl}/' + id,
      method: 'put',
      data: obj
    })
  },

  // 删除单条
  del4${entryLower}(id) {
    return request({
      url: serverName + '/${restUrl}/' + id,
      method: 'delete',
    })
  },


  // 删除多条
  dels4${entryLower}(ids) {
    return request({
      url: serverName + '/${restUrl}/dels',
      method: 'post',
      data: ids
    })
  },

  // 查询单条
  sel4${entryLower}(id) {
    return request({
      url: serverName + '/${restUrl}/' + id,
      method: 'get',
    })

  },

  // 下载Excel模板
  downloadExcelTemplate(params) {
    return request({
      url: serverName + '/${restUrl}/downloadExcelTemplate',
      method: 'post',
      data: params,
      responseType: 'arraybuffer'
    })
  },

  // 导入Excel接口URL
  uploadExcelUrl() {
    return request({
      url: serverName + '/${restUrl}/uploadExcel',
      type: 'form',
      method: 'post',
    })

  },
  // 导出Excel
  excelData4${entryLower}(params) {
    return request({
      url: serverName + '/${restUrl}/excel',
      method: 'post',
      data: params,
      responseType: 'arraybuffer'
    })
  }

}
